<header>
  <a class="edit">Edit</a>
</header>

<div class="content">
  <p><%= @data.title %></p>
  <ul>
  <% loadTimeApps = JSON.parse(localStorage.getItem("LoadTimeApp")) %>
  <% if loadTimeApps.length: %>
    <% for loadTimeApp in loadTimeApps: %>
      <li><%= loadTimeApp.data.title %></li>
    <% end %>
  <% else: %>
    <li>No apps</li>
  </ul>
</div>